{
  "description": "Given two events sent using REST endpoint, first one to subscription and second to DLQ, we should see one tasks produced",
  "enabled": true,
  "jurisdiction": "WA",
  "caseType": "WaCaseType",
  "options": {
    "taskRetrievalApi": "task-management-api"
  },
  "required": {
    "credentials": {
      "key": "WaSystemUser"
    },
    "ccd": [
      {
        "eventId": "CREATE",
        "caseData": {
          "template": "minimal-appeal-started.json"
        }
      }
    ]
  },
  "tests": [
    {
      "request": {
        "credentials": {
          "key": "WaSystemUser"
        },
        "input": {
          "eventMessages": [
            {
              "template": "minimal-ccd-event-message.json",
              "destination": "RestEndpoint",
              "replacements": {
                "EventId": "CREATE",
                "NewStateId": "TODO",
                "EventTimeStamp": "{$LOCAL_DATETIME_TODAY-1HOURS}",
                "AdditionalData": {
                  "Data": {
                    "appealType": "deprivation"
                  }
                }
              }
            },
            {
              "template": "minimal-ccd-event-message.json",
              "destination": "RestEndpointFromDLQ",
              "replacements": {
                "EventId": "submitAppeal",
                "NewStateId": "appealSubmitted",
                "AdditionalData": {
                  "Data": {
                    "appealType": "deprivation"
                  }
                }
              }
            }
          ]
        }
      },
      "expectations": [
        {
          "credentials": {
            "key": "WaCaseOfficer",
            "granularPermission": true
          },
          "status": 200,
          "numberOfTasksAvailable": 1,
